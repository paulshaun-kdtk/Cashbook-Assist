# Copy this file to .env and add your actual API keys

# Gemini AI API Key
# Get your free API key from: https://aistudio.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# RevenueCat API Keys
# Get your API keys from: https://app.revenuecat.com/
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=your_revenuecat_ios_api_key_here
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY=your_revenuecat_android_api_key_here

# Existing Appwrite Configuration
EXPO_PUBLIC_APPWRITE_PROJECT_ID=your_appwrite_project_id
EXPO_PUBLIC_APPWRITE_DATABASE_ID=your_appwrite_database_id
EXPO_PUBLIC_APPWRITE_API_KEY=your_appwrite_api_key
