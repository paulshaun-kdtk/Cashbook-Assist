{
  "expo": {
    "name": "Cashbook Assist - Manage your day to day cashflows",
    "slug": "cashbook-assist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "scheme": "cashbookassist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cashbookassist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "splash": {
      "image": "./assets/images/appIcon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-splash-screen",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "86ace278-9d7a-4a98-96ac-2da1c62862e3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/86ace278-9d7a-4a98-96ac-2da1c62862e3"
    },
    "android": {
      "package": "com.shuan_s.cashbookassist"
    }
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/appIcon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.shuan_s.cashbookassist"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/appIcon.png"
  },
  "plugins": [
    "expo-router",
    "expo-sqlite",
    "expo-web-browser"
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "86ace278-9d7a-4a98-96ac-2da1c62862e3"
    }
  },
  "runtimeVersion": {
    "policy": "appVersion"
  },
  "updates": {
    "url": "https://u.expo.dev/86ace278-9d7a-4a98-96ac-2da1c62862e3"
  }
}
